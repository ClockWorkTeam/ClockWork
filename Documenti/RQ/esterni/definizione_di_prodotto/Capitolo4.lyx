\begin_layout Itemize

\series bold
[void] endCall():
\series default
 metodo che interromperà 
\shape italic
localStream
\shape default
, chiuderà 
\shape italic
peerConnection
\shape default
 e lancerà l'evento 
\shape italic
setOnCall,
\shape default
 che NotificationCommunication raccoglierà per impostare la variabile 
\emph on
onCalling
\emph default
 a 
\emph on
false
\emph default
 e invocherà il metodo 
\shape italic
endcall(false)
\shape default
 di CallView
\end_layout

